    <!--===============================================================================================-->
    <title>Register</title>
  </head>
  <body class="antialiased font-poppins">


<!-- component -->
<div class="flex items-center justify-center min-h-screen p-6 bg-gray-100">
  <div class="container max-w-screen-lg mx-auto">
    <div>
     

      <div class="p-4 px-4 mb-6 bg-white rounded shadow-lg md:p-8">
        <div class="grid grid-cols-1 gap-4 text-sm gap-y-2 lg:grid-cols-3">
          <div class="text-gray-600">
            <p class="text-lg font-medium">Personal Details</p>
            <p>Please fill out all the fields.</p>
<div  style="margin-top: 100%;">
  <span class=" txt1 p-b-9"> Already have an account? </span>

  <a href="/login" class="txt3"> Login </a>
</div>

          </div>
         
           
       

          <div class="lg:col-span-2">
            <p style="text-align: center; color: rgb(190, 33, 33);"><%= msg %> </p>
            <form action="/register" method="post" enctype="multipart/form-data">
            <div class="grid grid-cols-1 gap-4 text-sm gap-y-2 md:grid-cols-5">
              <div class="md:col-span-5">
               
                <label for="firstname">First Name</label>
                <input type="text" name="FName" id="" class="w-full h-10 px-4 mt-1 border rounded bg-gray-50" value="" />
              </div>
              <div class="md:col-span-5">
               
                <label for="lastname">Last Name</label>
                <input type="text" name="LName" id="" class="w-full h-10 px-4 mt-1 border rounded bg-gray-50" value="" />
              </div>

              <div class="md:col-span-5">
                <label for="Phone">Phone Number</label>
                <input type="tel" name="PhoneNo" id="email" class="w-full h-10 px-4 mt-1 border rounded bg-gray-50" value="" />
              </div>

              <div class="md:col-span-5">
                <label for="email">Email</label>
                <input type="email" name="email" id="Email" class="w-full h-10 px-4 mt-1 border rounded bg-gray-50" value="" placeholder="email@domain.com" />
              </div>

              <div class="md:col-span-3">
                <label for="password">Password</label>
                <input type="password" name="Pass" id="" class="w-full h-10 px-4 mt-1 border rounded bg-gray-50" value="" placeholder="password" />
              </div>

              <div class="md:col-span-2">
                <label for="file">Choose File</label>
                <input type="file" name="Image" id="" class="w-full h-10 px-4 mt-1 border rounded bg-gray-50" accept="image/*" />
              </div>

             

            
              <div class="text-right md:col-span-5">
                <div class="inline-flex items-end">
                  <button class="px-4 py-2 font-bold text-white bg-red-500 rounded hover:bg-red-700">Register</button>
                </div>
              </div>
            </div>
            </form>
          </div>
        </div>
      </div>
    </div>

   
  </div>
</div>

























  
  
