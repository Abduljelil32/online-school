<body>

    <div class="limiter">

		<div class="container-login100">
			<div class="wrap-login100">
                <button onclick="history.back()" style="padding: 10px;"> 
                    <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="currentColor" class="bi bi-arrow-left" viewBox="0 0 16 16">
                        <path fill-rule="evenodd" d="M15 8a.5.5 0 0 0-.5-.5H2.707l3.147-3.146a.5.5 0 1 0-.708-.708l-4 4a.5.5 0 0 0 0 .708l4 4a.5.5 0 0 0 .708-.708L2.707 8.5H14.5A.5.5 0 0 0 15 8z"/>
                      </svg></button>
                <form action="/createCourse" method="POST" enctype="multipart/form-data" class="login100-form validate-form p-l-55 p-r-55 p-t-178">

                    <span class="login100-form-title">
						Add New Course
					</span>

                    <div class="wrap-input100 validate-input m-b-16" data-validate="Please enter course name">
						<input class="input100" type="text" name="courseName" id="courseName" placeholder="Enter Course Name" required>
						<span class="focus-input100"></span>
					</div>
                    <br><br>

                    <div class="wrap-input100 validate-input" data-validate = "Please enter course code">
						<input class="input100" type="text" name="courseCode" id="courseCode" placeholder="Enter Course Code" required>
						<span class="focus-input100"></span>
					</div>
                    <br><br>
                    <div class="wrap-input100 validate-input" data-validate = "Please enter course price">
						<input class="input100" type="number" name="price" id="price" placeholder="Enter Price" required>
						<span class="focus-input100"></span>
					</div>
                    <br><br>
                    <div class="container-login100-form-btn">
						<button class="login100-form-btn">
                            <input type="submit" value="Add Course">
						</button>
					</div>
					</div>
                </form>
            
           
			</div>
		</div>
	</div>




